    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "http-server": "^14.1.1",
    "parcel": "^2.13.3",
    "playwright": "^1.50.1",
    "prettier": "^3.5.0",
    "sass": "^1.84.0",
    "svgo": "^3.3.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
